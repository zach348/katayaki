<%= stylesheet_link_tag    'application', media: 'all' %>

<div>
  <h2>Welcome, <%= current_user.full_name %>!</h2>


</div>
  <h3/>Aspirations
    <ul>
      <% @aspirations.each do |aspiration| %>
        <div>
          <h5/> <%= link_to aspiration.goal.title, aspiration_path(aspiration) %> <br/>
          <p/> <%= aspiration.goal.description %>
        </div>
      <% end %>
    </ul>

<div>
  <h3/>Your Groups
  <ul>
    <% @groups.each do |group| %>
      <div>
        <h5/> <%= link_to group.name, group_path(group) %> <br/>
      </div>
    <% end %>
  </ul>

</div>

<div>

</div>
